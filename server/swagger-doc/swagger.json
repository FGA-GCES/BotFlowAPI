  "paths": {
    "/{projectName}/upload": {
      "post": {
        "summary": "Post a json with stories, intents and utters",
        "parameters": [
          {
            "in": "path",
            "name": "projectName",
            "type": "string",
            "required": true
          }
        ],
        "description": "Post a list containing an array of stories, intents and utters of an project",
        "responses": {
          "200": {
            "description": "A list of stories, intents and utters",
            "schema": {
              "type": "object",
              "required": [
                "intents",
                "utters",
                "stories"
              ],
              "properties": {
                "intents": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "type": "string"
                      },
                      "intentName": {
                        "type": "string"
                      },
                      "intent": {
                        "type": "string"
                      },
                      "entities": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "start": {
                              "type": "integer"
                            },
                            "end": {
                              "type": "integer"
                            },
                            "value": {
                              "type": "string"
                            },
                            "entity": {
                              "type": "string"
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "utters": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "type": "string"
                      },
                      "utterName": {
                        "type": "string"
                      },
                      "utter": {
                        "type": "string"
                      }
                    }
                  }
                },
                "stories": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "_id": {
                        "type": "string"
                      },
                      "utterName": {
                        "type": "string"
                      },
                      "intentName": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
